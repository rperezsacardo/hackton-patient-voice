<form class="sign-in-form" action="/authentication/sign-in" method="POST">
  <label for="input-email"></label>
  <input type="email" id="input-email" name="email" placeholder="User" />
  <label for="input-password"></label>
  <input type="password" name="password" id="input-password" placeholder="Password" />
  <button >Login</button>
</form>

